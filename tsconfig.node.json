{
  "compilerOptions": {
    "composite": true,
    "target": "ESNext",
    "module": "esnext",
    "moduleResolution": "Node",
    "tsBuildInfoFile": "./.cache/tsconfig.node.tsbuildinfo",
    "skipLibCheck": true,
    "lib": ["ESNext"],
    "types": [ "node" ],
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "noEmit": true
  },
  "include": [ "vite.config.ts", "package.json", "vite_plugins/**/*" ]

}
